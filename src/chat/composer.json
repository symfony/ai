{
    "name": "symfony/ai-chat",
    "description": "Low-level abstraction for triggering chat with an agent.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "chat",
        "agent",
        "store",
        "message"
    ],
    "authors": [
        {
            "name": "Christopher Hertel",
            "email": "mail@christopher-hertel.de"
        },
        {
            "name": "Oskar Stark",
            "email": "oskarstark@googlemail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "psr/log": "^3.0",
        "symfony/ai-agent": "@dev",
        "symfony/ai-platform": "@dev",
        "symfony/http-client": "^6.4 || ^7.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.0",
        "phpunit/phpunit": "^11.5",
        "psr/cache": "^3.0",
        "symfony/cache": "^6.4 || ^7.1",
        "symfony/http-foundation": "^6.4 || ^7.1"
    },
    "suggest": {
        "psr/cache": "To use any PSR-16 cache as a message store",
        "symfony/http-foundation": "To use the session as a message store"
    },
    "autoload": {
        "psr-4": {
            "Symfony\\AI\\Chat\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symfony\\AI\\PHPStan\\": "../../.phpstan/",
            "Symfony\\AI\\Chat\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "thanks": {
            "name": "symfony/ai",
            "url": "https://github.com/symfony/ai"
        }
    }
}
