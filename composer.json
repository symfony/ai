{
    "name": "symfony/ai",
    "description": "Symfony AI Monorepo",
    "license": "MIT",
    "keywords": [
        "dev"
    ],
    "require-dev": {
        "php": ">=8.2",
        "deptrac/deptrac": "^4.5",
        "php-cs-fixer/shim": "^3.91.2",
        "symfony/console": "^5.4|^6.4|^7.4|^8.0",
        "symfony/filesystem": "^5.4|^6.4|^7.4|^8.0",
        "symfony/finder": "^5.4|^6.4|^7.4|^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "conflict": {
        "phpstan/phpdoc-parser": "<2.0.0"
    }
}
